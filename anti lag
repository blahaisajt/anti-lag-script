--Loadstring: loadstring(game:httpGet("https://raw.githubusercontent.com/blahaisajt/anti-lag-script/main/anti%20lag"),true)()

function a()
	spawn(function()
		for _,v in pairs(workspace:GetDescendants()) do
			if v:IsA("Basepart") or v:IsA("Part") or v:IsA("MeshPart") then
				v.Material = Enum.Material.SmoothPlastic
			end
		end
	end)
	spawn(function()
		for _,v in pairs(workspace:GetDescendants()) do
			if v:IsA("Texture") or v:IsA("Decal") then
				v:Destroy()
			end
		end
	end)
end
a()
workspace.ChildAdded:Connect(function(Child)
	
	if Child:IsA("BasePart") or Child:IsA("Part") or Child:IsA("MeshPart") then
		Child.Material = Enum.Material.SmoothPlastic
	elseif Child:IsA("Texture") or Child:IsA("Decal") then
			Child:Destroy()
		end
	end
end)




